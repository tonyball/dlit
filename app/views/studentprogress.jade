script.
	$(document).ready(function(){
		$('.tooltipped').tooltip({delay: 50});
		$('select').material_select();
		$("#studentnametable").tablesorter();
		$('.collapsible').collapsible({ accordion : false });

		//- student's card detail events
		$('.stdDetailCard').hide();
		$('.stdDetailCard').first().show();
		$('.stdname').first().addClass('selected');
		var pre_card = '#cardDetailOf1';
		$('.stdname').click(function(){
			var card = '#cardDetailOf'+$(this).attr('id');
			if (pre_card == card){
				
			} else {
				$('.stdDetailCard').addClass('rollOut').hide(1000);
				$(card).show().removeClass('rollOut');
				$('.stdname').removeClass('selected');
				$(this).toggleClass('selected');
				pre_card = card;
			}
		});
	})